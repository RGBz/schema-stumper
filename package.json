{
  "name": "schema-stumper",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "pg": "^8.11.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "scripts": {
    "start": "npm run build:schema && react-scripts start",
    "build": "react-scripts build",
    "build:schema": "ts-node -O '{\"module\":\"CommonJS\"}' src/prepare-schema.ts",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/pg": "^8.11.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "react-scripts": "^0.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  }
}
